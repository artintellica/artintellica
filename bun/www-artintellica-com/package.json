{
  "name": "www-artintellica-com",
  "version": "0.1.0",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build:icons": "bun --bun process-icons.ts",
    "build:blog": "bun --bun build-blog.ts",
    "dev": "bun --bun react-router dev --port 4058",
    "build": "bun --bun cross-env NODE_ENV=production react-router build",
    "start": "bun --bun cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "bun --bun react-router typegen && bun --bun safe-routes typegen && tsc",
    "lint": "biome lint --write --unsafe",
    "format": "biome format --write",
    "fix": "bun run typecheck && bun run lint && bun run format"
  },
  "dependencies": {
    "@react-router/express": "^7.5.3",
    "@react-router/fs-routes": "^7.5.3",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "@tailwindcss/vite": "^4.1.7",
    "cross-env": "^7.0.3",
    "isbot": "^5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "safe-routes": "^1.0.12",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@react-router/dev": "^7.5.3",
    "@types/bun": "latest",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.3",
    "autoprefixer": "^10.4.21",
    "feed": "^5.0.1",
    "png-to-ico": "^2.1.8",
    "retext": "^9.0.0",
    "retext-smartypants": "^6.2.0",
    "sharp": "^0.34.1",
    "tailwindcss": "^4",
    "toml": "^3.0.0",
    "vite": "^6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
